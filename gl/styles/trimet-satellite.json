    {
      "id": "poi_label",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "poi",
      "minzoom": 14,
      "maxzoom": 24,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["==", "rank", 1],
        ["!in", "class", "bus", "railway"]
      ],
      "layout": {
        "icon-size": 1,
        "text-anchor": "top",
        "text-field": "{name:latin}\n{name:nonlatin}",
        "text-font": ["Source Sans Pro Light"],
        "text-max-width": 8,
        "text-offset": [0, 0.5],
        "text-size": 11,
        "visibility": "visible",
        "text-transform": "uppercase",
        "text-letter-spacing": 0.1
